	"tokenColors": [
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.modifier",
				"storage.type"
			],
			"settings": {
				"foreground": "#ff515e",
			}
		},
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#625d69",
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#b5a5ff",
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#ff5e6a",
			}
		},
		{
			"name": "Booleans",
			"scope": "constant.language",
			"settings": {
				"foreground": "#ff5e6a"
			}
		},
		{
			"name": "Methods",
			"scope": [
				"entity.name.function",
				"support.function",
			],
			"settings": {
				"foreground": "#7b6397",
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.class",
				"meta.tag",
			],
			"settings": {
				"foreground": "#ffc454"
			}
		},
		{
			"name": "Properties",
			"scope": [
